{
  "name": "Chil",
  "version": "0.1.31",
  "description": "Libreria para escribir pruebas de aceptacion al estilo Gherkin",
  "keywords": "test, pruebas, bdd, tdd, acceptance, gherkin",
  "repository": {
    "type": "git",
    "url": "https://github.com/joacomf/chil.git"
  },
  "authors": [
    {
      "name": "Joaquin Moreno",
      "email": "joaquinamf@gmail.com",
      "maintainer": true
    }
  ],
  "license": "MIT",
  "homepage": "https://joacomf.github.io/chil/",
  "dependencies": {},
  "frameworks": "*",
  "platforms": "*",
  "examples": [
    {
      "name": "LEDs",
      "base": "ejemplos/leds",
      "files": [
        "pruebas/.gitignore",
        "pruebas/platformio.ini",
        "pruebas/src/pasos.h",
        "pruebas/src/main.cpp",
        "pruebas/src/pasos.cpp",
        "pruebas/.chil",
        "fuente/platformio.ini",
        "fuente/.gitignore",
        "fuente/src/main.cpp",
        "README.md"
      ]
    },
    {
      "name": "Baliza IC",
      "base": "ejemplos/baliza-ic",
      "files": [
        "pruebas/.gitignore",
        "pruebas/platformio.ini",
        "pruebas/src/pasos.h",
        "pruebas/src/main.cpp",
        "pruebas/src/pasos.cpp",
        "pruebas/src/respuestas.h",
        "pruebas/.chil",
        "README.md"
      ]
    }
  ],
  "export": {
    "include": [
      "src/*",
      "CMakeLists.txt",
      "README.md",
      "version",
      "ejemplos/*"
    ],
    "exclude": [
      ".github",
      "documentacion",
      "herramientas",
      "scripts",
      "ejemplos/baliza-ic/imagenes",
      "ejemplos/baliza-ic/fuente",
      "ejemplos/leds/imagenes",
      "tests"
    ]
  }
}
